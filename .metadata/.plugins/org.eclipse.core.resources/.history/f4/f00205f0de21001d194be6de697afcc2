package com.cetpainfotech.day4;

public class Person {
	
	private int id;
	private String name;
	
	private int age;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Person(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	public Person() {
		super();
	}
	
	
	
	
	
	public void printDetails()
	{
		System.out.println("ID = "+id+" Name ="+name);
	}
	
	

}
