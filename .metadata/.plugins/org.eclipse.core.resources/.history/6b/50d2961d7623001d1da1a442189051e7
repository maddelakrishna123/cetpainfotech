package com.cetpainfotech.day6;

public class Demo {
	public static void main(String[] args) {
		
		
		Bank b = new HDFCBank();
		
		
		int roi =b.getROI();
		System.out.println("The Rate of Intrest given  is :"+roi);
		
		b = new ICICIBank();
		 roi =b.getROI();
			System.out.println("The Rate of Intrest given  is :"+roi);
			b = new SBIBank();
			 roi =b.getROI();
				System.out.println("The Rate of Intrest given  is :"+roi);
	}

}
