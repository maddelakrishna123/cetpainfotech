package com.cetpainfotech.day4;

public class Person {
	
	protected int id;
	protected String name;
	
	protected int age;
	
	
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Person(int id, String name, int age) {
		super(); // call to super class constructor
		this.id = id;
		this.name = name;
		this.age = age;
		System.out.println("In Person class All argument construcor");
	}
	
	  public Person() { 
		  super();
	  
	  System.out.println("In person class default constructor"); }
	 
	
	
	
	
	
	public double printDetails()
	{
		System.out.println("ID = "+id+" Name ="+name+" Age ="+age);
		
		return 0;
	}
	
	

}
