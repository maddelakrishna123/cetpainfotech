package com.cetpainfotech.day9;

import java.util.Scanner;

import com.cetpainfotech.day4.Employee;

public class ExceptionDemo5 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		String emp = sc.nextLine();
		
		String[] detatils = emp.split(",");
		
		int age= Integer.parseInt(detatils[3]);
		try
		{
		 if(age<23 || age>50)
		 {
			InvalidAgeException iae = new InvalidAgeException("Invalid age"); 
		
		  throw iae;
		 
		 }
		}catch (InvalidAgeException e) {
			System.out.println("Invalid Age Exception");
		}
		Employee e = new Employee(0, null, 0, 0);
	}

}
