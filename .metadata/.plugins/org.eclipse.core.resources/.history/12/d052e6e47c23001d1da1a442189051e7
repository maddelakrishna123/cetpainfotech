package com.cetpainfotech.day6;

public class InerfaceDemo {
	public static void main(String[] args) {
		
		Speaker s = new Teacher();
		
		s.speak();
		
		
		
		Speaker s1 = new Speaker() {

			@Override
			public void speak() {
				System.out.println("anonymous speaker");
				
			}
			
			
			
		};
	}

}
