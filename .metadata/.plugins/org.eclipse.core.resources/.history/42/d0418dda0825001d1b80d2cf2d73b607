package com.cetpainfotech.day8;

public class StringDemo1 {

	public static void main(String[] args) {
		
		
		
		// p - p 
		
		double x =90.0;
		int y =(int) x;
		System.out.println(y);
		
		
		// String - p
		
		
	//	java.lang
		// byte -  Byte 
		
		// short - Short
		
		// int  - Integer
		
		// long  - Long
		
		// float - Float
		
		// double - Double
		
		// boolean  - Boolean
		
		// char - Character
		
		
		String st1 = "123";
		
		// int - Integer
		
	int st3=	Integer.parseInt(st1);
		String str2 =" 234.45";
		
		// double - Double
		
		Double.parseDouble(str2);
		
		
		
		
		int num = 123; // convert this into string
		
		String num1 = String.valueOf(123.45);
		
		
	}

}
